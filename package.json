{
  "private": true,
  "name": "dr-react",
  "version": "0.0.2-dev.1",
  "author": "dr-js",
  "license": "MIT",
  "description": "A collection of strange functions, with React",
  "keywords": [
    "Dr",
    "Dr-js",
    "JavaScript"
  ],
  "homepage": "https://github.com/dr-js/dr-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dr-js/dr-react.git"
  },
  "bugs": {
    "url": "https://github.com/dr-js/dr-react/issues"
  },
  "scripts": {
    "// script ======================": "",
    "script-pack": "cross-env BABEL_ENV=dev babel-node ./script pack",
    "script-publish": "cross-env BABEL_ENV=dev babel-node ./script pack publish",
    "script-publish-dev": "cross-env BABEL_ENV=dev babel-node ./script pack publish-dev",
    "script-generate-export": "cross-env BABEL_ENV=dev babel-node ./script/generateExport",
    "// build =======================": "",
    "build-module": "cross-env NODE_ENV=production BABEL_ENV=module babel ./source --out-dir ./output-gitignore/module --ignore source/index.example.js",
    "build-example": "cross-env BABEL_ENV=dev babel-node ./script/webpack production",
    "build-example-dev": "cross-env BABEL_ENV=dev babel-node ./script/webpack development",
    "// minify =======================": "",
    "minify-module": "cross-env BABEL_ENV=dev babel-node ./script/uglify module",
    "// =============================": "",
    "prepack": "echo \"Error: should pack with repack-*\" && exit 1"
  },
  "engines": {
    "node": ">=8.8.0",
    "npm": ">=5.5.0"
  },
  "peerDependencies": {
    "dr-js": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "styled-components": "*"
  },
  "devDependencies": {
    "dev-dep-tool": "^0.0.14-dev.7",
    "dev-dep-web-react-styled-components": "^0.0.14-dev.6",
    "dr-js": "^0.12.0-dev.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
