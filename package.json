{
  "private": true,
  "name": "dr-react",
  "version": "0.0.2-dev.7",
  "description": "A collection of strange functions, with React",
  "author": "dr-js",
  "license": "MIT",
  "keywords": [
    "Dr",
    "Dr-js",
    "JavaScript"
  ],
  "repository": "github:dr-js/dr-react.git",
  "scripts": {
    "// script ======================": "",
    "script-base": "babel-node ./script quiet",
    "script-pack": "babel-node ./script quiet pack",
    "script-publish": "babel-node ./script pack publish",
    "script-publish-dev": "babel-node ./script pack publish-dev",
    "script-generate-spec": "babel-node ./script/generateSpec",
    "// build =======================": "",
    "build-module": "cross-env BABEL_ENV=module babel ./source --out-dir ./output-gitignore/module --ignore source/index.example.js",
    "build-example": "babel-node ./script/webpack production profile",
    "build-example-dev": "babel-node ./script/webpack development watch",
    "// =============================": "",
    "analyze-stat": "npx webpack-bundle-analyzer .temp-gitignore/profile-stat.json",
    "prepack": "echo \"Error: should pack with repack-*\" && exit 1"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "peerDependencies": {
    "dr-js": "0.19.0-dev.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "dr-dev": "0.0.2-dev.1",
    "dr-dev-web-react-styled-components": "0.0.2-dev.1",
    "dr-js": "0.19.0-dev.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "sideEffects": false
}
